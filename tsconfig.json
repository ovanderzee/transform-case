{
    "compilerOptions": {
        /* Visit https://aka.ms/tsconfig.json to read more about this file */

        /* Basic Options */
        "target": "ES6",
        "module": "ES6",

        /* Strict Type-Checking Options */
        "strict": true,

        /* Module Resolution Options */
        "esModuleInterop": true,

        /* Advanced Options */
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
