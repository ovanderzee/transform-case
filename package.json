{
    "name": "transform-case",
    "version": "0.1.0",
    "description": "Transforms casing and spacing of a string",
    "main": "dist/transformCase.js",
    "module": "module/transformCase.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "prebuild": "clear-folder dist module && prettier './{src,test}/*.js' --write",
        "build": "rollup --config",
        "coverage": "clear-folder coverage && jest --coverage",
        "once": "npm run test && npm run build",
        "pretest": "eslint {src,test}/*.js",
        "test": "jest",
        "watch": "onchange --initial \"{src,test}/*.js\" -- npm run once --silent"
    },
    "keywords": [
        "convert",
        "transform",
        "camel",
        "camelcase",
        "camel-case",
        "title",
        "titlecase",
        "title-case",
        "snake",
        "snakecase",
        "snake-case",
        "case",
        "front-end"
    ],
    "author": "Onno van der Zee",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ovanderzee/transform-case.git"
    },
    "bugs": {
        "url": "https://github.com/ovanderzee/transform-case/issues"
    },
    "homepage": "https://github.com/ovanderzee/transform-case#readme",
    "dependencies": {
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "fold-to-ascii": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "clear-folder": "^1.1.2",
        "coveralls": "^3.0.11",
        "eslint": "^6.5.1",
        "eslint-plugin-jasmine": "^2.10.1",
        "eslint-plugin-jest": "^22.20.0",
        "jest": "^25.1.0",
        "onchange": "^6.1.0",
        "prettier": "^1.18.2",
        "rollup": "^1.32.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-terser": "^5.3.0"
    }
}
